{
  "manifest_version": 2,

  "name": "TubeMarks for Youtube",
  "description": "A fun extension",
  "version": "1.2",
  "browser_action": {
    "default_icon": "img/icon16.png",
    "default_popup": "popup.html"
  },
  "icons": { "16": "img/icon16.png",
           "48": "img/icon48.png",
          "128": "img/icon128.png" },
  "content_scripts":[
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["jquery-3.1.0.min.js", "myscript.js"]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
   ]
}